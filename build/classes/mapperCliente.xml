<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dawi">
	<resultMap type="ClienteIbatis" id="resultClienteConsulta">
		<id column="cod_cli" property="codigo"/>
		<result column="nom_cli" property="nombres"/>
		<result column="ape_cli" property="apellidos"/>
		<result column="dni_cli" property="dni"/>
		<result column="sexo_cli" property="sexo"/>
	</resultMap>
	<select id="SQL_listarClientesXSexo" statementType="CALLABLE" 
			parameterType="string"	resultMap="resultClienteConsulta">
		call sp_listClientesXSexo(#{vSexo});
	</select>
	
	
</mapper>



